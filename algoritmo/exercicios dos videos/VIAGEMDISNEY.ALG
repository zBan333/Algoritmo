algoritmo "ViagemDisney"
var
   dinheiro: real
inicio
      escreva("Quanto de dinheiro que voce tem para ir pra disney? ")
      leia(dinheiro)
      se (dinheiro >= 1000) entao
         escreva("Eba voce pode ir para a disney!")
      senao
           escreva("Infelizmente voce n pode ir para a disney")
      FimSE

fimalgoritmo