algoritmo "Nome do Programa"
var
    v: vetor[1..6] de inteiro
    contador: inteiro
inicio
    para contador <- 1 ate 6 faca
        escreva("Digite o ", contador,"Â° valor: ")
        leia(valor[contador])
    fimpara
    para contador <- 1 ate 6 faca
        escreva("[", valor[contador], "]")
    fimpara
fimalgoritmo