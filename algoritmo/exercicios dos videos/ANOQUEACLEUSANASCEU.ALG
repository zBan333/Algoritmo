Algoritmo "AnoQueACleusaNasceu"

Var
    anoAtual, anoNascimento, idade: real
  


Inicio
      escreva("em que ano estamos?")
     leia(anoAtual)
     escreva("em que ano eu/você nasceu?")
     leia(anoNascimento)
     idade <- anoAtual - anoNascimento
     escreva("a sua idade é de ", idade, " anos de idade")



Fimalgoritmo