algoritmo "Nome do Programa"
var
    valor, contador, resultado: inteiro
inicio
    escreva("Digite um valor para ver sua tabuada ")
    leia(valor)

    para contador <- 1 ate 10 passo 1 faca
        resultado <- valor * contador
        escreval(valor," X ", Contador, " = ", resultado)
    fimpara
fimalgoritmo