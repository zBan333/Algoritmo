Algoritmo "exercicio15"

Var
    diasTrabalhados, horasTrabalhadas, salarioTotal: inteiro

Inicio
      escreva("quantos dias voce trabahlou neste mes: ")
      leia(diasTrabalhados)
      
      horasTrabalhadas <- (diasTrabalhados * 8)
      salarioTotal <- (horasTrabalhadas * 25)
      
      escreva("o seu salario é de ", salarioTotal, " reais" )
      


Fimalgoritmo