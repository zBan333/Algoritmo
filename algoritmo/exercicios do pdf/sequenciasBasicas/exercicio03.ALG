algoritmo "Exercicio03"
var
   Nome: caractere
   Salario: real
inicio
      Escreva("Informe o nome do funcionario: ")
      leia(Nome)
      Escreva("Informe o salario: ")
      leia(Salario)
      Escreva("O funcionario ", Nome, " tem um salario de ", Salario, " reais")
      
      

fimalgoritmo