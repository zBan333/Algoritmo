algoritmo "exercicio09"
var
   dinheiro, taxaDollar, dollarQueTem: real

inicio
      escreva("Informe quanto de dinheiro que voce tem: ")
      leia(dinheiro)
      taxaDollar <- 3.45
      dollarQueTem <- (dinheiro / taxaDollar)
      escreva("Você tem aproximadamente ", dollarQueTem, " dolares")
      

fimalgoritmo