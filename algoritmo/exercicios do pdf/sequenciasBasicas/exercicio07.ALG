algoritmo "Exercicio07"
var
   numero, dobro, terceiraParte: real

inicio
      Escreva("Digite um numero: ")
      leia(numero)
      dobro <- (numero * 2)
      terceiraParte <- (numero / 3)
      escreval("O dobro do numero ", numero, " é ", dobro)
      escreva("A terceira parte do numero ", numero, " é", terceiraParte)
      
      

fimalgoritmo