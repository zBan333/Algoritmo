algoritmo "Exerecicio06"
var
   numero, antecessor, sucessor: real

inicio
      Escreva("Digite um numero: ")
      leia(numero)
      antecessor <- (numero - 1)
      sucessor <- (numero + 1)
      Escreva("O antecessor do numero ", numero, " é ", antecessor, " e o sucessor é ", sucessor)
      

fimalgoritmo